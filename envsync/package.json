{
  "name": "@bennbatuu/envsync",
  "version": "1.0.0",
  "description": "Effortless Environment Variable Management for Developers - Sync, manage, compare, and backup .env files across multiple environments with CLI and web interface",
  "main": "dist/cli/index.js",
  "bin": {
    "envsync": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:studio": "cd studio && npm run dev",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "environment",
    "variables",
    "env",
    "sync",
    "cli",
    "developer-tools",
    "dotenv",
    "environment-management",
    "configuration",
    "backup",
    "encryption",
    "web-interface",
    "studio",
    "devops",
    "deployment"
  ],
  "author": "Batuhan (bennbatuu)",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "diff": "^5.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/diff": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bennbatuu/open-source.git",
    "directory": "envsync"
  },
  "homepage": "https://github.com/bennbatuu/open-source#readme",
  "bugs": {
    "url": "https://github.com/bennbatuu/open-source/issues"
  }
}
