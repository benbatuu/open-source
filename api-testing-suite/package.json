{
  "name": "api-testing-suite",
  "version": "1.0.0",
  "description": "A comprehensive API testing suite with automated testing, monitoring, and documentation generation",
  "main": "dist/index.js",
  "bin": {
    "api-test": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docs:generate": "typedoc src --out docs",
    "cli": "ts-node src/cli/index.ts"
  },
  "keywords": [
    "api",
    "testing",
    "automation",
    "jest",
    "supertest",
    "typescript",
    "mock",
    "performance",
    "monitoring"
  ],
  "author": "bennbatuu.com",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "supertest": "^6.3.3",
    "msw": "^2.0.11",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "table": "^6.8.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benbatuu/open-source.git",
    "directory": "api-testing-suite"
  },
  "bugs": {
    "url": "https://github.com/benbatuu/open-source/issues"
  },
  "homepage": "https://github.com/benbatuu/open-source#readme"
}
